<%= render 'shared/errors', resource: @faculties %>

<h1>Listing Faculties</h1>

<table class="table">
  <thead>
    <tr>
      <th>Emp</th>
      <th>Ip committee</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faculties.each do |faculty| %>
      <tr>
        <td><%= faculty.emp_id %></td>
        <td><%= faculty.ip_committee %></td>
        <td><%= faculty.user %></td>
        <td><%= link_to 'Show', faculty %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Faculty', new_faculty_path %>
